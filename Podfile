# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'GoChat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  #When you add a new pod '' package like pod 'Firebase/Auth' cd GoChat and run 'pod install' in command line
  use_frameworks!
    pod 'Firebase'
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
  # Pods for GoChat

end
